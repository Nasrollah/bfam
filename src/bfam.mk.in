BFAM_VERSION             = @bfam_VERSION@
BFAM_CC                  = @MPI_C_COMPILER@
BFAM_CXX                 = @MPI_CXX_COMPILER@
BFAM_CFLAGS              = @CMAKE_C_FLAGS@
BFAM_CFLAGS_DEBUG        = @CMAKE_C_FLAGS_DEBUG@
BFAM_CFLAGS_RELEASE      = @CMAKE_C_FLAGS_RELEASE@
BFAM_INCLUDES            = -I@CMAKE_INSTALL_PREFIX@/include
BFAM_LIBRARIES           = -lbfam -lp4est -lsc -locca -llua -lz -ldl -lm
BFAM_LDFLAGS             = @CMAKE_EXE_LINKER_FLAGS@ -L@CMAKE_INSTALL_PREFIX@/lib -Wl,-rpath -Wl,@CMAKE_INSTALL_PREFIX@/lib
BFAM_CUDA_INCLUDE_DIRS   = @CUDA_INCLUDE_DIRS@
BFAM_OPENCL_INCLUDE_DIRS = @OPENCL_INCLUDE_DIRS@
BFAM_HAVE_CUDA           = @BFAM_HAVE_CUDA@
BFAM_HAVE_OPENCL         = @BFAM_HAVE_OPENCL@
BFAM_HAVE_OCCA           = @BFAM_HAVE_OCCA@
